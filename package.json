{
  "name": "redux-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "npm run config -- dev && ng serve --environment=dev",
    "rimraf": "rimraf"
  },
  "engines": {
    "node": ">= 5.0",
    "npm": ">= 3"
  },
  "dependencies": {
    "@angular-redux/router": "7.0.0",
    "@angular-redux/store": "6.4.1",
    "@angular/animations": "6.0.0",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "angular-bootstrap-md": "6.0.2",
    "chart.js": "2.5.0",
    "core-js": "2.5.4",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "redux": "3.6.0",
    "redux-logger": "3.0.6",
    "redux-observable": "0.14.1",
    "rxjs": "6.0.0",
    "rxjs-compat": "6.1.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.0",
    "@angular/cli": "6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "@types/redux": "3.6.0",
    "codelyzer": "4.2.1",
    "jasmine-core": "2.99.1",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "1.7.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "5.3.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@angular-redux/store)"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/index.ts",
      "!src/**/*.type.ts",
      "!src/**/*.module.ts",
      "!src/**/*.model.ts",
      "!src/**/*.page.ts",
      "!src/vendor.ts",
      "!src/main.ts",
      "!src/polyfills.ts",
      "!src/store/epics/epic-providers.ts",
      "!src/store/epics/root.epic.ts",
      "!src/store/reducers/root-reducer.ts",
      "!src/store/store-providers.ts",
      "!src/store/middlewares/middlewares.ts"
    ],
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setup-jest.ts",
    "testRegex": "\\.spec\\.ts$"
  }
}
